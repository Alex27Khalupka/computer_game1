nrel_climbed

<-sc_node_norole_relation;

<-  antisymmetric_relation;
<|- reflexive_relation;
<|- transitive_relation;
<- oriented_relation;
	 =>nrel_first_domain: operative;
		nrel_second_domain: supporting_wall;
=> nrel_main_idtf:     
	 [забираться*]   (*<-lang_ru;;*);
      [climbed*]  (*<-lang_en;;*); 

