nrel_deactivation

<-  sc_node_norole_relation;
<-  antisymmetric_relation;
<|- reflexive_relation;
<|- transitive_relation;
<- oriented_relation;
	 =>nrel_first_domain: operative;
		nrel_second_domain: charge;
=> nrel_main_idtf:     
	 [деактивация*]   (*<-lang_ru;;*);
      [deactivation*]  (*<-lang_en;;*); 

<- rrel_key_sc_element: ... (*
    <- definition;;
    => nrel_main_idtf:
      [Опр. (деактивация*)] (* <- lang_ru;; *);;
 => nrel_using_constants: {
charge;
rrel_attack_operative
    };;
    <= nrel_sc_text_translation: ... (*
      -> rrel_example:
[Деактивация заряда - процесс отключения  <b><sc_element sys_idtf="charge" > заряда </b></sc_element>  после чего следует победа <b><sc_element sys_idtf="rrel_attack_operative" > атаки </b></sc_element> ](*<-lang_ru;;  => nrel_format: format_html;;*);;
      *);;
  *);;

